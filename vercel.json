{
    "version": 2,
    "routes": [
      { "src": "/vhd", "dest": "/" },
      { "src": "/_next/(.*)", "dest": "/vhd/_next/$1" },
      { "src": "/api/.*", "dest": "/express-server/routes/route.js" }
    ],
    "builds": [
      { "src": "/vhd/package.json", "use": "@vercel/next" },
      { "src": "express-server/server.js", "use": "@vercel/node" }
    ]
  }